{
  "name": "@choragen/eslint-plugin",
  "version": "0.0.1",
  "description": "ESLint rules for agentic development traceability and quality",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "agentic",
    "traceability"
  ],
  "license": "MIT",
  "author": "Justin Neal",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  }
}
