{
  "name": "@choragen/core",
  "version": "0.0.1",
  "description": "Core task chain, governance, and coordination primitives",
  "keywords": [
    "agentic",
    "tasks",
    "governance",
    "coordination"
  ],
  "license": "MIT",
  "author": "Justin Neal",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tasks": {
      "types": "./dist/tasks/index.d.ts",
      "import": "./dist/tasks/index.js"
    },
    "./governance": {
      "types": "./dist/governance/index.d.ts",
      "import": "./dist/governance/index.js"
    },
    "./locks": {
      "types": "./dist/locks/index.d.ts",
      "import": "./dist/locks/index.js"
    },
    "./protocol": {
      "types": "./dist/protocol/index.d.ts",
      "import": "./dist/protocol/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "import": "./dist/config/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.9"
  }
}
